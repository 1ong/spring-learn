<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="beanFactoryProcessorHandler" class="com.learn.spring.beans.configuration.BeanFactoryPPLifeCycleHandler"/>

    <!-- 预计设置名称为Python,beanFactoryProcessorHandler可进行修改 -->
    <bean id="userInfo" class="com.learn.spring.beans.entity.UserInfo">
        <property name="name" value="Python"/>
        <property name="age" value="26"/>
    </bean>
</beans>