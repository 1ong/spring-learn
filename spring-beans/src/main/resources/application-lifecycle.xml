<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="beanLifeCycle" class="com.learn.spring.beans.lifecycle.BeanLifeCycle"/>

    <bean id="beanFactoryPostProcessorLifeCycle" class="com.learn.spring.beans.lifecycle.BeanFactoryPostProcessorLifeCycle"/>

    <bean id="beanPostProcessorLifeCycle" class="com.learn.spring.beans.lifecycle.BeanPostProcessorLifeCycle"/>
</beans>